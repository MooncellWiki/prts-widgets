{
  "name": "mooncell-micro-fontend",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "sourcemap": "source-map-explorer ./dist/*.js",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint ./cli ./src --ext .mjs,.js,.ts,.jsx,.tsx,.vue --quiet --fix "
  },
  "dependencies": {
    "@sentry/browser": "^7.14.0",
    "@sentry/tracing": "^7.14.0",
    "@vicons/material": "^0.12.0",
    "@vueuse/core": "^9.1.0",
    "@webank/eslint-config-ts": "^1.1.1",
    "hammerjs": "^2.0.8",
    "md5": "^2.3.0",
    "naive-ui": "^2.32.1",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@types/hammerjs": "^2.0.41",
    "@types/md5": "^2.3.2",
    "@types/offscreencanvas": "^2019.7.0",
    "@types/tough-cookie": "^4.0.2",
    "@vitejs/plugin-vue": "^3.0.3",
    "got": "^12.5.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "rollup-plugin-visualizer": "^5.8.0",
    "source-map-explorer": "^2.5.2",
    "terser": "^5.14.2",
    "tough-cookie": "^4.1.2",
    "typescript": "^4.6.4",
    "vite": "^3.0.7",
    "vite-plugin-windicss": "^1.8.7",
    "vue-tsc": "^0.39.5",
    "windicss": "^3.5.6"
  },
  "lint-staged": {
    "*.{mjs,js,ts,vue,jsx,tsx}": [
      "pnpm run lint:fix"
    ]
  },
  "browserslist": [
    "chrome > 70",
    "edge > 81",
    "firefox > 70",
    "safari > 12",
    "ios_saf > 12",
    "not opera > 0",
    "not samsung > 0",
    "not and_qq > 0"
  ]
}